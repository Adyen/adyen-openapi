                        }
                     },
                     "schema" : {
                        "$ref" : "#/components/schemas/CostEstimateRequest"
                     }
                  }
               }
            },
            "responses" : {
               "200" : {
                  "content" : {
                     "application/json" : {
                        "examples" : {
